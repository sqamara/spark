mkdir outputs
for ((j=8;j<10;j++)) # determines the power of 10
do
  for ((i=0;i<10;i++))
  do
    bin/spark-submit --master spark://SMBP:7077 my_dev/test/FiveByTwo/target/scala-2.10/five-by-two-project_2.10-1.0.jar $j | grep "memTransfer == " |  cut -c 16- >> output
  done
  
  NORM="outputs/output_norm_${j}"
  CHOS="outputs/output_chos_${j}"

  awk 'NR%2==1' output > $NORM
  awk 'NR%2==0' output > $CHOS

  rm output
done
